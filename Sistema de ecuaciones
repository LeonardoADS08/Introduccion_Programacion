#include "stdafx.h"
#include <iostream>
#include <string>
using namespace std;
int main()
{
	float a, b, c, d, e, f, x, y;
	cout << "Ingrese los coeficientes:" << endl;
	cout << "Primera ecuacion:" << endl;
	cin >> a >> b >> c;
	cout << "Segunda ecuacion:" << endl;
	cin >> d >> e >> f;
	x = ( (c+e) * e ) / (a*e + d*b);
	y = (f - d * x) / e;
	cout << "El valor de x es: " << x << endl;
	cout << "El valor de y es: " << y << endl;
	system("PAUSE");
	return 0;
}

